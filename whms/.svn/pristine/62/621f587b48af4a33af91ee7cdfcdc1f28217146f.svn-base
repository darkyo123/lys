{
    -webkit-appearance: none;
    -moz-appearance: none;
    -o-appearance: none;
    appearance: none;
    outline-width:0;
    font-size:13px;
}
select::-ms-expand {
    display: none;
}
select[multiple]
{
    vertical-align:top;
}
input[type=text]::-ms-clear {
    display:none;
}
.select{display:inline-block;height:30px;position:relative;border:1px solid #d1d1d1;background:#fff;box-sizing:border-box;}
.select:after{content:'';display:block;width:0;height:0;border-top:8px solid #3e4358;border-bottom:8px solid transparent;border-right:5px solid transparent;border-left:5px solid transparent;position:absolute;right:10px;top:40%;}
.select select{height:28px;padding:0 .5em;box-sizing:border-box;width:100%;line-height:28px;cursor: pointer;color:#666;}

.input{vertical-align:middle;height:30px;border:1px solid #d1d1d1;background:#fff;box-sizing:border-box;transition: all 0.3s ease-out}
.input input{height:28px;padding:5px;box-sizing:border-box;color:#666;}
.input input:focus {outline: none;background-color: transparent;border-color:#21acdb}

/* btn-type -------------------------------------------------------------------*/
.btn-type {display:inline-block;position:relative;cursor:pointer;text-align:center;transition:all 0.3s; font-size:0;}
.btn-type.btn1 {height:40px;line-height:34px;padding:0 40px;font-size:16px;letter-spacing:-0.03em;text-transform:uppercase;font-family: 'NotoSansKR'; font-style: normal; font-weight: 400; border-radius:24px;box-sizing: border-box;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;}
.btn-type.btn1.black {color:#fff !important;border:1px solid #262729;background:#262729}
.btn-type.btn1.dgray {color:#fff !important;border:1px solid #4f535c;background:#4f535c}
.btn-type.btn1.gray {color:#fff !important;border:1px solid #7c8288;background:#7c8288}
.btn-type.btn1.lgray {color:#fff !important;border:1px solid #9da3aa;background:#9da3aa}
.btn-type.btn1.blue {color:#fff !important;border:1px solid #0f62c0;background:#0f62c0}
.btn-type.btn1.lblue {color:#fff !important;border:1px solid #02afc9;background:#02afc9}
.btn-type.btn1.green {color:#fff !important;border:1px solid #33bb7b;background:#33bb7b}
.btn-type.btn1.lgreen {color:#262729 !important;border:1px solid #56e79a;background:#56e79a}
.btn-type.btn1.orange {color:#fff !important;border:1px solid #e88d33;background:#e88d33}
.btn-type.btn1.yellow {color:#262729 !important;border:1px solid #feda10;background:#feda10;font-weight: 500;}
.btn-type.btn1.white{color:#fff !important;border:1px solid #fff;background:none}
.btn-type.btn1.red {color:#fff !important;border:1px solid #e02e44;background:#e02e44}
.btn-type.btn1.black_wline {color:#000 !important;border:1px solid #000;background:#fff}
.btn-type.btn1.orange_line {color:#ff862a !important;border:1px solid #ff862a;background:#fff}
.btn-type.btn1.black_line {color:#000 !important;border:1px solid #000;background-color: transparent; }
.btn-type.btn1.ngray{color:#fff !important;border:1px solid #93979e;background:#93979e}
.btn-type.btn1.bgray{color:#fff !important;border:1px solid #5d5b59;background:#5d5b59}

