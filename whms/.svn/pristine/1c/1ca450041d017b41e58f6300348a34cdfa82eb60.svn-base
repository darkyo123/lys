				AND MAKE_DT = #{targetDt}
			</when>
			<otherwise>
				AND MAKE_DT = (SELECT MAX(MAKE_DT) FROM HLTH_QSLT_INF WHERE USE_GRP_CD = hqi.USE_GRP_CD AND USR_ID = hqi.USR_ID)
			</otherwise>
		</choose>
