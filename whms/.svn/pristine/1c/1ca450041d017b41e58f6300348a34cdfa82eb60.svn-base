	<select id="selectNewUsrId" resultType="String">
		SELECT CONCAT(DATE_FORMAT(NOW(),"%y%m%d"), LPAD( IFNULL((SELECT CAST(SUBSTR(MAX(USR_ID),7,6) AS unsigned)+1 FROM WHMS_USR_MST),1) , "6", '0') ) usrId
    </select>

