										<div class="roll-content" <c:if test="${status.count == fn:length(detailVO.monthList)}">style='display: block'</c:if> >
											<div class="table type3">
												<table>
													<colgroup>
														<col width="100">
														<col width="100">
														<col width="90">
														<col width="90">
														<col width="auto">
													</colgroup>
													<tbody>
														<c:forEach var="detail" items="${detailVO.detailList}" varStatus="status">
